{
    "name": "sails-react-bootstrap-webpack",
    "version": "4.0.1",
    "description": "An opinionated base configuration of Sails, with Webpack for React support, and Bootstrap for styling.",
    "keywords": [
        "sails",
        "react",
        "bootstrap",
        "webpack",
        "mocha",
        "chai",
        "react-bootstrap",
        "monorepo"
    ],
    "dependencies": {
        "csrf": "3.1.0",
        "json-stringify-safe": "5.0.1",
        "lodash": "4.17.21",
        "moment-timezone": "0.5.40",
        "prop-types": "15.8.1",
        "sails": "1.5.4",
        "sails-hook-orm": "4.0.2",
        "sails-hook-sockets": "2.0.3",
        "sails-mysql": "3.0.0",
        "scrypt-kdf": "2.0.1",
        "serve-static": "1.15.0",
        "superagent": "8.0.9"
    },
    "devDependencies": {
        "@babel/core": "7.20.12",
        "@babel/eslint-parser": "7.19.1",
        "@babel/preset-env": "7.20.2",
        "@babel/preset-react": "7.18.6",
        "@popperjs/core": "2.11.6",
        "babel-loader": "9.1.2",
        "bootstrap": "5.2.3",
        "chai": "4.3.7",
        "chai-spies": "1.0.0",
        "chai-uuid": "1.0.6",
        "copy-webpack-plugin": "11.0.0",
        "core-js": "3.28.0",
        "css-loader": "6.7.3",
        "eslint": "8.34.0",
        "eslint-plugin-react": "7.32.2",
        "favicons": "7.1.0",
        "favicons-webpack-plugin": "6.0.0",
        "file-loader": "6.2.0",
        "fixted": "4.1.0",
        "html-webpack-plugin": "5.5.0",
        "mini-css-extract-plugin": "2.7.2",
        "mkdirp": "2.1.3",
        "mocha": "10.2.0",
        "npm-run-all": "4.1.5",
        "nyc": "15.1.0",
        "react": "18.2.0",
        "react-bootstrap": "v2.7.2",
        "react-dom": "18.2.0",
        "react-router-dom": "6.8.1",
        "react-transition-group": "4.4.5",
        "rimraf": "4.1.2",
        "sass": "1.58.3",
        "sass-loader": "13.2.0",
        "style-loader": "3.3.1",
        "supertest": "6.3.3",
        "webpack": "5.75.0",
        "webpack-cli": "5.0.1",
        "webpack-dev-server": "4.11.1",
        "webpack-merge": "5.8.0"
    },
    "scripts": {
        "coverage": "npm run lint && NODE_ENV=testing nyc --reporter=html mocha",
        "test": "npm run lint && npm run custom-tests && echo 'Done.'",
        "lint": "./node_modules/eslint/bin/eslint.js . --max-warnings=0 --report-unused-disable-directives && echo 'âœ”  Your .js files look good.'",
        "custom-tests": "NODE_ENV=testing mocha",
        "start": "run-p open:client lift",
        "start:debug": "npm-run-all --parallel open:client debug",
        "open:client": "webpack-dev-server --mode development --open --config webpack/dev.config.js",
        "build": "run-s clean build:prod",
        "build:dev": "run-s clean build:dev:webpack",
        "build:dev:webpack": "webpack --mode development --config webpack/dev-build.config.js",
        "build:prod": "NODE_ENV=production webpack --mode production --config webpack/prod.config.js",
        "clean": "rimraf .tmp && mkdirp .tmp/public",
        "lift": "node app.js",
        "lift:prod": "NODE_ENV=production node app.js",
        "debug": "node --inspect app.js",
        "lines": "git ls-files --exclude-standard -- ':!:**/*.[pjs][npv]g' ':!:**/*.ai' ':!:.*' ':!:**/.*' ':!:CHANGELOG.md' ':!:**/*.md' ':!:*.md' ':!:test/coverage/*' ':!:assets/dependencies/*' ':!:package.json' ':!:config/*' ':!:LICENSE' ':!:package-lock.json' | xargs wc -l"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/neonexus"
        }
    ],
    "main": "app.js",
    "repository": {
        "type": "git",
        "url": "git://github.com/neonexus/sails-react-bootstrap-webpack.git"
    },
    "author": "NeoNexus DeMortis",
    "license": "Unlicense",
    "engines": {
        "node": ">=18.14"
    }
}

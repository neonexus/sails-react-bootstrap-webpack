{
    "name": "sails-react-bootstrap-webpack",
    "version": "2.0.0",
    "description": "An opinionated base configuration of Sails, with Webpack for React support, and Bootstrap for styling.",
    "keywords": [
        "sails",
        "react",
        "bootstrap",
        "webpack",
        "mocha",
        "chai",
        "react-bootstrap"
    ],
    "dependencies": {
        "@popperjs/core": "^2.10.2",
        "bootstrap": "^5.1.3",
        "copy-webpack-plugin": "^9.0.1",
        "csrf": "^3.1.0",
        "json-stringify-safe": "^5.0.1",
        "lodash": "^4.17.21",
        "md5": "^2.3.0",
        "moment-timezone": "^0.5.33",
        "prop-types": "^15.7.2",
        "react": "^17.0.2",
        "react-bootstrap": "^v2.0.0",
        "react-dom": "^17.0.2",
        "react-router-dom": "^5.3.0",
        "react-transition-group": "^4.4.2",
        "sails": "^1.5.0",
        "sails-hook-orm": "^4.0.0",
        "sails-hook-sockets": "^2.0.1",
        "sails-mysql": "^2.0.0",
        "sass": "^1.43.3",
        "scrypt-kdf": "^2.0.1",
        "serve-static": "^1.14.1",
        "superagent": "^6.1.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@babel/core": "^7.15.8",
        "@babel/preset-env": "^7.15.8",
        "@babel/preset-react": "^7.14.5",
        "@babel/eslint-parser": "^7.16.3",
        "babel-loader": "^8.2.3",
        "chai": "^4.3.4",
        "chai-spies": "^1.0.0",
        "chai-uuid": "^1.0.6",
        "core-js": "^3.18.3",
        "css-loader": "^6.4.0",
        "eslint": "^8.1.0",
        "eslint-plugin-react": "^7.26.1",
        "favicons-webpack-plugin": "^5.0.2",
        "file-loader": "^6.2.0",
        "fixted": "^3.0.2",
        "html-webpack-plugin": "^5.4.0",
        "mini-css-extract-plugin": "^2.4.3",
        "mocha": "^9.1.3",
        "npm-run-all": "^4.1.5",
        "nyc": "^15.1.0",
        "rimraf": "^3.0.2",
        "sass-loader": "^12.2.0",
        "style-loader": "^3.3.1",
        "supertest": "^6.1.6",
        "webpack": "^5.59.1",
        "webpack-cli": "^4.9.1",
        "webpack-dev-server": "^4.3.1",
        "webpack-merge": "^5.8.0"
    },
    "scripts": {
        "coverage": "NODE_ENV=testing nyc --reporter=html mocha",
        "test": "npm run lint && npm run custom-tests && echo 'Done.'",
        "lint": "./node_modules/eslint/bin/eslint.js . --max-warnings=0 --report-unused-disable-directives && echo 'âœ”  Your .js files look good.'",
        "custom-tests": "NODE_ENV=testing mocha",
        "start": "run-p open:client lift",
        "start:debug": "npm-run-all --parallel open:client debug",
        "open:client": "webpack-dev-server --mode development --open --config webpack/dev.config.js",
        "build": "run-s clean build:prod",
        "build:dev": "run-s clean build:dev:webpack",
        "build:dev:webpack": "webpack --mode development --config webpack/dev-build.config.js",
        "build:prod": "NODE_ENV=production webpack --mode production --config webpack/prod.config.js",
        "clean": "rimraf .tmp && mkdirp .tmp/public",
        "lift": "node app.js",
        "debug": "node --inspect app.js",
        "lines": "git ls-files --exclude-standard -- ':!:**/*.[pjs][npv]g' ':!:**/*.ai' ':!:.idea' ':!:test/coverage/*' ':!:.*ignore' ':!:*.md' ':!:assets/dependencies/*' ':!:package.json' ':!:config/*' ':!:.babelrc' ':!:.editorconfig' ':!:*.eslintrc' ':!:.mocharc.yaml' ':!:.npmrc' ':!:.nycrc' ':!:.sailsrc' ':!:LICENSE' ':!:package-lock.json' | xargs wc -l"
    },
    "main": "app.js",
    "repository": {
        "type": "git",
        "url": "git://github.com/neonexus/sails-react-bootstrap-webpack.git"
    },
    "author": "NeoNexus DeMortis",
    "license": "Unlicense",
    "engines": {
        "node": "^14.8.1"
    }
}

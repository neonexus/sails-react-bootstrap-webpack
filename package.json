{
    "name": "sails-react-bootstrap-webpack",
    "version": "3.0.3",
    "description": "An opinionated base configuration of Sails, with Webpack for React support, and Bootstrap for styling.",
    "keywords": [
        "sails",
        "react",
        "bootstrap",
        "webpack",
        "mocha",
        "chai",
        "react-bootstrap",
        "monorepo"
    ],
    "dependencies": {
        "csrf": "3.1.0",
        "json-stringify-safe": "5.0.1",
        "lodash": "4.17.21",
        "md5": "2.3.0",
        "moment-timezone": "0.5.35",
        "prop-types": "15.8.1",
        "sails": "1.5.3",
        "sails-hook-orm": "4.0.2",
        "sails-hook-sockets": "2.0.3",
        "sails-mysql": "3.0.0",
        "scrypt-kdf": "2.0.1",
        "serve-static": "1.15.0",
        "superagent": "8.0.0",
        "uuid": "8.3.2"
    },
    "devDependencies": {
        "@babel/core": "7.18.10",
        "@babel/eslint-parser": "7.18.9",
        "@babel/preset-env": "7.18.10",
        "@babel/preset-react": "7.18.6",
        "@popperjs/core": "2.11.6",
        "babel-loader": "8.2.5",
        "bootstrap": "5.1.3",
        "chai": "4.3.6",
        "chai-spies": "1.0.0",
        "chai-uuid": "1.0.6",
        "copy-webpack-plugin": "11.0.0",
        "core-js": "3.24.1",
        "css-loader": "6.7.1",
        "eslint": "8.22.0",
        "eslint-plugin-react": "7.30.1",
        "favicons": "6.2.2",
        "favicons-webpack-plugin": "5.0.2",
        "file-loader": "6.2.0",
        "fixted": "3.0.4",
        "html-webpack-plugin": "5.5.0",
        "mini-css-extract-plugin": "2.6.1",
        "mocha": "10.0.0",
        "npm-run-all": "4.1.5",
        "nyc": "15.1.0",
        "react": "18.2.0",
        "react-bootstrap": "v2.5.0",
        "react-dom": "18.2.0",
        "react-router-dom": "6.3.0",
        "react-transition-group": "4.4.5",
        "rimraf": "3.0.2",
        "sass": "1.54.4",
        "sass-loader": "13.0.2",
        "style-loader": "3.3.1",
        "supertest": "6.2.4",
        "webpack": "5.74.0",
        "webpack-cli": "4.10.0",
        "webpack-dev-server": "4.10.0",
        "webpack-merge": "5.8.0"
    },
    "scripts": {
        "coverage": "NODE_ENV=testing nyc --reporter=html mocha",
        "test": "npm run lint && npm run custom-tests && echo 'Done.'",
        "lint": "./node_modules/eslint/bin/eslint.js . --max-warnings=0 --report-unused-disable-directives && echo 'âœ”  Your .js files look good.'",
        "custom-tests": "NODE_ENV=testing mocha",
        "start": "run-p open:client lift",
        "start:debug": "npm-run-all --parallel open:client debug",
        "open:client": "webpack-dev-server --mode development --open --config webpack/dev.config.js",
        "build": "run-s clean build:prod",
        "build:dev": "run-s clean build:dev:webpack",
        "build:dev:webpack": "webpack --mode development --config webpack/dev-build.config.js",
        "build:prod": "NODE_ENV=production webpack --mode production --config webpack/prod.config.js",
        "clean": "rimraf .tmp && mkdirp .tmp/public",
        "lift": "node app.js",
        "lift:prod": "NODE_ENV=production node app.js",
        "debug": "node --inspect app.js",
        "lines": "git ls-files --exclude-standard -- ':!:**/*.[pjs][npv]g' ':!:**/*.ai' ':!:.*' ':!:**/.*' ':!:CHANGELOG.md' ':!:**/*.md' ':!:*.md' ':!:test/coverage/*' ':!:assets/dependencies/*' ':!:package.json' ':!:config/*' ':!:LICENSE' ':!:package-lock.json' | xargs wc -l"
    },
    "main": "app.js",
    "repository": {
        "type": "git",
        "url": "git://github.com/neonexus/sails-react-bootstrap-webpack.git"
    },
    "author": "NeoNexus DeMortis",
    "license": "Unlicense",
    "engines": {
        "node": ">=16.14"
    }
}
